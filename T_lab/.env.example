# =============================================================================
# T_lab Environment Variables
# =============================================================================

# OpenAI API Key (Required for LLM agents)
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (Optional, for enhanced web search)
TAVILY_API_KEY=your_tavily_api_key_here

# Database (PostgreSQL)
DATABASE_URL=postgresql://tlab:tlab_password@localhost:5432/tlab_db

# Redis (Celery broker)
REDIS_URL=redis://localhost:6379/0

# Server Config
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=false
